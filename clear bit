#include <stdio.h>

int main(){
    //declaring & initializing an 1 byte number
    
    unsigned char num = 0xAA; 
    printf("num = %02X\n", num);
    //clearing 3rd bit 
    num &= ~(1<<3);
    //now value will be: 1010 0110 = 0xA6 (HEX) = 166 (DEC)
    printf("num = %02X\n", num);    
    return 0;    
		}
