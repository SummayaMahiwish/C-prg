/**
This program returns the size of a 2 Dimension array
*/

# include <stdio.h>

int main(void)
{
    
    int** arr[][20];
    
    //printf("size of array = %d\n",sizeof(arr));
     //printf("size of array = %d\n",sizeof(int));
      printf("size of array = %d\n",sizeof(arr)/sizeof(int)); // THIS IS CORRECT 20 X 20 = 400
       //printf("size of array = %d\n",sizeof(arr)-sizeof(int));
}
